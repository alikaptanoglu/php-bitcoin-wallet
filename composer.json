{
  "name": "andkom/php-bitcoin-wallet",
  "description": "A simple PHP library for reading Bitcoin wallet.dat file.",
  "type": "library",
  "keywords": [
    "bitcoin",
    "wallet",
    "wallet.dat"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrei Komarov"
    }
  ],
  "require": {
    "php": "^7.1",
    "andkom/php-berkeley-db": "^1.1",
    "andkom/php-bcdatastream": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.0"
  },
  "autoload": {
    "psr-4": {
      "AndKom\\Bitcoin\\Wallet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndKom\\Bitcoin\\Wallet\\Tests\\": "tests/"
    }
  }
}
